<div class="card overlayedCard tpl_03 isRel {{ page_type|default }}" {% if item.sheet_id %} data-sheetid="{{item.sheet_id}}" {% else %} data-postid="{{ item.post_id }}" {% endif %}>
    {% if item.link.url %}
        {% if obfuscation %}
            <span class="card-link obf" data-obf="{{ item.link.url|base64Encode }}" data-target="{{ item.link.target|default('_self') }}">
            {% else %}
                <a class="card-link" href="{{ item.link.url }}" target="{{ item.link.target|default('_self') }}">
                {% endif %}

            {% endif %}
            {{ block('card_media_wrapper', 'cards/basic_card/tpl_01/tpl.twig') }}
            <div class="card-section fullcenterbox fcbCol bgimg-op-85">
                {{ block('card_terms', 'cards/basic_card/tpl_01/tpl.twig') }}
                {{ block('card_titles', 'cards/basic_card/tpl_01/tpl.twig') }}
                {% if item.booking.link %}
                    <span class="card-bookable">{{ __('RÃ©servable', 'woody_theme') }}</span>
                {% endif %}
            </div>
            {% if obfuscation %}
            </span>
    {% elseif item.link.url %}
        </a>
    {% endif %}
</div>
