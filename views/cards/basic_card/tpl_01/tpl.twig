<div class="card basicCard {{ index_class|default('tpl_01')}} isRel {{ page_type|default }}{{ item.slideshow ? ' has-slideshow' }} {{ item.card_class|default }}{{ item.link.url ? '' : ' has-not-card-link' }}" {% if item.sheet_id %} data-sheetid="{{item.sheet_id}}" {% else %} data-postid="{{ item.post_id }}" {% endif %}>
    {% if display_index and card_index %}
        <div class="isAbs card-index">
            {{ card_index|zeroise(2) }}
        </div>
    {% endif %}
    {% if item.link.url %}
        {% if obfuscation %}
            <span class="card-link obf" data-obf="{{ item.link.url|base64Encode }}" data-target="{{ item.link.target|default('_self') }}">
        {% else %}
            <a class="card-link" href="{{ item.link.url }}" target="{{ item.link.target|default('_self') }}" {% if item.datahref%}data-href="{{  item.datahref }}"{% endif %}>
        {% endif %}
            {% endif %}
            {% block card_media_wrapper %}
                {% if image_style and (item.img.ID or item.img.url) %}
                    {% include '_objects/imageObject.twig' with {
                    'img' : item.img,
                    'image_style' : image_style,
                    'containerClass' : "cardMediaWrapper",
                    'noRespCrop' : "noRespCrop",
                    'hide_caption' : true
                } %}
                {% elseif item.movie %}
                    {% include '_objects/videoObject.twig' with {
                    'movie' : item.movie,
                    'image_style' : image_style,
                    'containerClass' : "cardMediaWrapper",
                    'autoplay': true,
                    'muted' : true,
                    'loop': true
                } %}
                {% endif %}
            {% endblock %}
            <div class="card-section{{ card_section_class|default }} {% if item.profil %}has-profile{% endif %}" {% if not desequalize %} data-equalizer-watch {% endif %}>
                {% block card_terms %}
                    {% if item.terms %}
                        <div class="card-terms flex-container align-center wrap">
                            {% for term_key, term in item.terms %}
                                {% if term.name|default %}
                                    <span class="card-term {{ term_key }}-term {{ term.slug|default }}" data-term-id="{{ term.term_id|default }}">
                                        {{ term.name|default }}
                                    </span>
                                {% endif %}
                            {% endfor %}
                        </div>
                    {% endif %}
                {% endblock %}
                {% block card_titles %}
                    {% include '_objects/titlesObject.twig' with {
                        'titles' : item,
                        'contextPrefix' : "card",
                        'ellipsis' : ellipsis|default(180)
                    } %}
                {% endblock %}
                {% block card_profile %}
                    {% if item.profil.img or item.profil.name %}
                        <div class="card-profile">
                            {% if item.profil.img %}
                                {% include '_objects/imageObject.twig' with {
                                    'img' : item.profil.img,
                                    'image_style' : 'ratio_square',
                                    'containerClass' : "profile-img",
                                    'hide_caption' : true
                                } %}
                            {% endif %}
                            {% if item.profil.name %}
                                <span class="profile-name">{{ item.profil.name }}</span>
                            {% endif %}
                        </div>
                    {% endif %}
                {% endblock %}
                {% block card_button %}
                    {% if display_button and (item.link.link_label or item.link.title) %}
                        <span class="card-button button">
                            {% if item.link.link_label %}
                                {{ item.link.link_label }}
                            {% elseif item.link.title %}
                                {{ item.link.title }}
                            {% endif %}
                        </span>
                    {% endif %}
                {% endblock %}
                {% block card_bookable %}
                    {% if item.booking.link %}
                        <span class="card-bookable">{{ __('RÃ©servable', 'woody_theme') }}</span>
                    {% endif %}
                {% endblock %}
            </div>
    {% if obfuscation %}
        </span>
    {% elseif item.link.url %}
        </a>
    {% endif %}
</div>
